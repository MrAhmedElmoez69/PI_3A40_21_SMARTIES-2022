{% extends 'base_back.html.twig' %}

{% block title %}Abonnement{% endblock %}


{% block main %}
    <h1>Abonnement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abonnement.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ abonnement.type }}</td>
            </tr>
            <tr>
                <th>Dated</th>
                <td>{{ abonnement.dated ? abonnement.dated|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Datef</th>
                <td>{{ abonnement.datef ? abonnement.datef|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ abonnement.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('abonnement_index') }}">back to list</a>

    <a href="{{ path('abonnement_edit', {'id': abonnement.id}) }}">edit</a>

    {{ include('abonnement/_delete_form.html.twig') }}
{% endblock %}
